}

/* - Home ADS in boxed mode -*/



#boxed-style ul.masonry_list li.homeadv,

#boxed-style ul.masonry_list li.homeadv iframe,

#boxed-style ul.masonry_list li.homeadv embed,

#boxed-style ul.masonry_list li.homeadv object,

